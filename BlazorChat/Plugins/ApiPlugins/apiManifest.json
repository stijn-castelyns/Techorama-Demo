{
  "applicationName": "U2U Course API Plugin",
  "description": "This plugin accesses the U2U Course API",
  "publisher": {
    "name": "Stijn",
    "contactEmail": "stijn@u2u.be"
  },
  "apiDependencies": {
    "mycourseapi": {
